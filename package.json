  "dependencies": {
    "@fortawesome/fontawesome-free-regular": "^5.0.13",
    "@fullhuman/postcss-purgecss": "^4.0.2",
    "axios": "^0.21.1",
    "bootstrap": "^4.6.0",
    "bootstrap-icons": "^1.4.0",
    "express": "^4.17.1",
    "font-awesome": "^4.7.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-bootstrap": "^1.5.1",
    "react-dom": "^17.0.1",
    "react-rating": "^2.0.5"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.13.9",
    "@babel/preset-env": "^7.13.5",
    "@babel/preset-react": "^7.12.13",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.4.1",
    "babel-core": "^6.26.3",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.1.0",
    "enzyme": "^3.11.0",
    "eslint": "^7.20.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "expect": "^26.6.2",
    "file-loader": "^6.2.0",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "nodemon": "^2.0.7",
    "react-test-renderer": "^17.0.1",
    "style-loader": "^2.0.0",
    "webpack": "^5.24.2",
    "webpack-cli": "^4.5.0"
  },
  "scripts": {
    "start": "node server/index.js",
    "start:dev": "npx nodemon server/index.js -w server",
    "build:dev": "webpack -w",
    "test": "node_modules/jest/bin/jest.js",
    "tailwind:build": "NODE_ENV=production postcss client/src/tailwind.css -o src/tailwind.output.css",
    "tailwind:dev": "postcss client/src/tailwind.css -o client/src/tailwind.output.css",
    "prestart": "npm run tailwind:dev",
    "prebuild": "npm run tailwind:build"
  },
  "jest": {
    "verbose": true,
    "testPathIgnorePatterns": [
      "/node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  }
}
